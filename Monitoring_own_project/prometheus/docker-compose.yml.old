version: '3.8'
services:
  prometheus:
    image: prom/prometheus:v2.34.0
    container_name: prometheus
    command:
      - '--config.file=/etc/prometheus/prometheus.yml'
    ports:
      - 9090:9090
    restart: unless-stopped
    volumes:
      - ./prometheus:/etc/prometheus.yml
      - prom_data:/prometheus
volumes:
  prom_data:
